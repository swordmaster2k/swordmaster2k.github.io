pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
gun_sprite = 1
player_sprite = 2

player_x = 64
player_y = 100

items = {}
item_start = 16
item_count = 8
item_interval = 16

gravity = 1
level = 1
points = 0

ticks = 0
last = 0

bullets = {}
ammo = 6
reloading = false

title_screen_text = {"reef invader", "press b"}
title_screen = true
game_over_text = {"game over", "you failed the reef dude!", "press a to try again"}
game_over = false
flash_text = true

function _init()
    for i = 1, level do
        item = {
            sprite = flr(rnd(item_count) + item_start),
            x = flr(rnd(120) + 5),
            y = i * (-item_interval)
        }
        add(items, item)
    end
end

function _update()
    if title_screen == true then
      if btn(4) then title_screen = false end
    elseif game_over == true then
      if btn(4) and time() - last > 1 then  run() end
    else
        if btn(0) and player_x - 2 > 0 then
         player_x -= 2
        end
        if btn(1) and player_x + 2 < 120 then
         player_x += 2
        end
        if btnp(4, 0) and ammo > 0 then
          add(bullets, bulletconstruct(player_x, player_y - 16))
          sfx(2)
          ammo -= 1
          if ammo < 1 then last = time() end
        end

        if ammo < 1 then
         if time() - last > 0.8 then
          ammo = 6
          last = 0
         end
        end

        for bullet in all(bullets) do bullet.update(bullet) end
        for item in all(items) do check_collisions(item) end

        if #items == 0 then
            level += 1
            _init()
        end
    end

end

function check_collisions(item)
 item.y += gravity
 for bullet in all(bullets) do
   if  item.y + 4 > bullet.y - 8
   and item.y + 4 < bullet.y + 8
   and item.x + 8 > bullet.x
   and item.x + 8 < bullet.x + 16 then
     points += 1
     del(items, item)
     del(bullets, bullet)
   end
   if bullet.y < 1 then
     del(bullets, bullet)
   end
 end

 if item.y > 100 then
   if mget(item.x / 8, (item.y - 100) / 8) != 0 then
     del(items,item)
     mset(item.x / 8, (item.y - 100) / 8, 0)
   end
 end

 if item.y > 116 then
   del(items, item)
   last = time()
   game_over = true
   return
 end
end

function _draw()
    cls()
    if title_screen == true then draw_title_screen()
    elseif game_over == true then draw_game_over()
    else draw_game_play() end
end

bulletconstruct = function(x, y)
 return {x = x, y = y, sprite = 27, update = function(this) this.y -= 6 end}
end

function hcenter(s)
  return 64 - #s * 2
end

function vcenter(s)
  return 61
end

function flash(text, increment)
 if flash_text == true then
  print(text, hcenter(text), vcenter(text) + #text * increment, 10)
 end
 ticks = (ticks + 1) % 16
 flash_text = (ticks < 8)
end

function draw_title_screen()
 print(title_screen_text[1], hcenter(title_screen_text[1]), vcenter(title_screen_text[1]), 8)
 flash(title_screen_text[2], 2)
end

function draw_game_play()
 map(0, 0, 0, 108, 16, 3)
 spr(player_sprite, player_x, player_y)
 spr(gun_sprite, player_x, player_y - 8)

 for item in all(items) do spr(item.sprite, item.x, item.y) end
 for bullet in all(bullets) do spr(bullet.sprite, bullet.x, bullet.y) end

 print("score: "..points, 0, 0, 8)

 ammo_text = ""
 for i = 1, ammo do ammo_text = ammo_text .. "|" end
 print(ammo_text, 100, 0, 10)
 if ammo < 1 then flash("RELOADING", 0) end
end

function draw_game_over()
 print(game_over_text[1], hcenter(game_over_text[1]), vcenter(game_over_text[1]), 8)
 print("score: "..points, hcenter("score: "..points), vcenter("score: "..points) + #game_over_text[2], 8)
 print(game_over_text[2], hcenter(game_over_text[2]), vcenter(game_over_text[2]) + #game_over_text[2] * 2, 8)
 flash(game_over_text[3], 3)
end

